@TECHREPORT{MSU-CSE-02-26,
AUTHOR = {J. Weng and Y. Zhang},
TITLE = {A Mentally Developing Robot and Its Thinking},
NUMBER = {MSU-CSE-02-26},
INSTITUTION = {Computer Science and Engineering, Michigan State University},
ADDRESS = {East Lansing, Michigan},
ABSTRACT = {The muddiness of AI tasks motivates autonomous mental development (AMD) robots, robots that "live" in
human environments to develop their minds. The Self-Aware and Self-Effecting (SASE) agent model explains why a traditional
AI program does not truly know what it does. The E/I-I architecture models how to realize SASE. We further show how the
Self-organizing Autonomous Incremental Learner (SAIL) AMD robot is able to think in a way closer to human thinking than ever
possible before. Such a thinking capability enables the SAIL robot to perform a series of muddy tasks, e.g., online introduction
and recognition of dynamic objects through interactive speech dialogue. Further, SAIL autonomously learned task transfer "on the
fly," using its autonomously generated "trains of thought." A computational cost analysis indicates that AMD robots that have
human-brain storage size (hard disks) and produce human-speed sensory response rate are within the reach of the current PC
technology.},
KEYWORDS = {Autonomous mental development, autonomous robots, artificial intelligence, vision, speech, language, mind},
NOTE = {},
MONTH = {October},
YEAR = {2002},
}

